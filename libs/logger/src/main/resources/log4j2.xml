<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info" packages="com.dispatch.logger">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <CustomJsonLayout/>
        </Console>
    </Appenders>
    <Loggers>
        <logger name="org.quartz" level="off"> </logger>
        <Root level="INFO">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
